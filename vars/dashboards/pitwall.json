        {
          "dataset": "iox",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH FuelUsagePerLap AS (\n    SELECT\n        \"lap\",\n        CASE\n            WHEN selector_first(\"FuelLevel\", \"time\")['value'] - selector_last(\"FuelLevel\", \"time\")['value'] < 0 THEN 0\n            ELSE selector_first(\"FuelLevel\", \"time\")['value'] - selector_last(\"FuelLevel\", \"time\")['value']\n        END AS \"FuelPerLap\"\n    FROM\n        \"telemetry_data\"\n    WHERE\n        $__timeFilter(\"time\") AND \"lap\" > 0\n    GROUP BY\n        \"lap\"\n)\nSELECT\n    AVG(\"FuelPerLap\") AS \"Avg\",\n    MIN(CASE WHEN \"FuelPerLap\" > 0 THEN \"FuelPerLap\" ELSE NULL END) AS \"Min\",\n    MAX(\"FuelPerLap\") AS \"Max\"\nFROM\n    FuelUsagePerLap;",
          "refId": "A",
