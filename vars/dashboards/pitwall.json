          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n    lap,\n    CASE\n        WHEN fuel_change < 0 THEN 0\n        ELSE fuel_change\n    END AS fuel_per_lap\nFROM\n    (\n        SELECT\n            lap,\n            selector_first(\"FuelLevel\", \"time\")['value'] - selector_last(\"FuelLevel\", \"time\")['value'] AS fuel_change\n        FROM\n            \"telemetry_data\"\n        WHERE\n            time >= $__timeFrom() AND time <= $__timeTo()\n            AND lap > 0\n        GROUP BY\n            lap\n    )\nORDER BY lap DESC\nLIMIT 5",
          "refId": "A",
