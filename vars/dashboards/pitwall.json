          "rawQuery": true,
          "rawSql": "WITH fuel_usage AS (\n    SELECT\n        lap,\n        CASE\n            WHEN selector_first(\"FuelLevel\", \"time\")['value'] - selector_last(\"FuelLevel\", \"time\")['value'] < 0 THEN 0\n            ELSE selector_first(\"FuelLevel\", \"time\")['value'] - selector_last(\"FuelLevel\", \"time\")['value']\n        END AS fuel_per_lap\n    FROM\n        \"telemetry_data\"\n    WHERE\n        time >= $__timeFrom() AND time <= $__timeTo()\n        AND \"lap\" > 0\n    GROUP BY\n        lap\n),\nfuel_needed AS (\n    SELECT\n        GREATEST(0, (selector_first(\"SessionTimeRemain\", \"time\")['value'] / mean(\"LapLastLapTime\"))) * mean(fuel_usage.fuel_per_lap) AS total_fuel_needed\n    FROM\n        \"telemetry_data\",\n        fuel_usage\n    WHERE\n        time >= $__timeFrom() AND time <= $__timeTo()\n)\nSELECT\n    ceil(total_fuel_needed / $liters) AS \"pitstops_remaining\"\nFROM\n    fuel_needed",
          "refId": "A",
